v0x6000002b82d0_0 .var "reset", 0 0;
E_0x6000025b9840 .event posedge, v0x6000002b8000_0;
S_0x12f604cc0 .scope module, "uut" "counter_nb" 2 10, 3 1 0, S_0x12f604b50;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /OUTPUT 4 "q";
P_0x6000025b9880 .param/l "N" 0 3 1, +C4<00000000000000000000000000000100>;
v0x6000002b8000_0 .net "clk", 0 0, v0x6000002b81b0_0;  1 drivers
v0x6000002b8090_0 .var "q", 3 0;
v0x6000002b8120_0 .net "reset", 0 0, v0x6000002b82d0_0;  1 drivers
E_0x6000025b9900 .event posedge, v0x6000002b8120_0, v0x6000002b8000_0;
    .scope S_0x12f604cc0;
T_0 ;
    %wait E_0x6000025b9900;
    %load/vec4 v0x6000002b8120_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x6000002b8090_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x6000002b8090_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x6000002b8090_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x12f604b50;
T_1 ;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x6000002b81b0_0;
    %inv;
    %store/vec4 v0x6000002b81b0_0, 0, 1;
    %jmp T_1.0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x12f604b50;
T_2 ;
    %vpi_call 2 21 "$dumpfile", "waveform_counter_nbit.vcd" {0 0 0};
    %vpi_call 2 22 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x12f604b50 {0 0 0};
    %vpi_call 2 24 "$monitor", "Time =%0t | clk=%b | reset=%b | q=%4b", $time, v0x6000002b81b0_0, v0x6000002b82d0_0, v0x6000002b8240_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6000002b81b0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6000002b82d0_0, 0, 1;
    %wait E_0x6000025b9840;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6000002b82d0_0, 0, 1;
    %pushi/vec4 18, 0, 32;
T_2.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_2.1, 5;
    %jmp/1 T_2.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x6000025b9840;
    %jmp T_2.0;
T_2.1 ;
    %pop/vec4 1;
    %vpi_call 2 37 "$finish" {0 0 0};
    %end;
    .thread T_2;
